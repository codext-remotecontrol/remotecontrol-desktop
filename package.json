    "mocha": "9.1.3",
    "ngx-tailwind": "2.2.2",
    "npm-run-all": "4.1.5",
    "rxjs": "7.4.0",
    "spectron": "15.0.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "10.4.0",
    "tslib": "2.3.1",
