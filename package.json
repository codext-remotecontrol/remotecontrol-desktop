{
  "name": "remotecontrol-desktop",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://codext.de",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "child_process": false,
    "stream": false
  },
  "author": {
    "name": "Codext GmbH",
    "email": "info@codext.de"
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node ./scripts/fix.script.js",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run electron:serve-tsc && cross-env NODE_ENV=production ng build --prod",
    "ng:serve": "ng serve -c web --host 0.0.0.0",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:mac": "npm run build:prod && electron-builder build --mac --publish=never",
    "electron:linux": "npm run build:prod && electron-builder build --linux --publish=never",
    "electron:windows": "npm run build:prod && electron-builder build --windows --publish=never",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "rebuild": "electron-rebuild -f -t prod,optional,dev -w robotjs"
  },
  "dependencies": {
    "@electron/remote": "1.0.4",
    "@ionic/angular": "^5.6.3",
    "@ionic/core": "^5.6.3",
    "@ionic/storage": "^3.0.2",
    "@sweetalert2/ngx-sweetalert2": "9.0.0",
    "@types/simple-peer": "9.6.2",
    "build-number-generator": "1.0.0",
    "electron-drag": "2.0.0",
    "electron-util": "github:dertieran/electron-util",
    "mac-screen-capture-permissions": "1.1.0",
    "ngx-electron": "2.2.0",
    "replace-in-files-cli": "1.0.0",
    "robotjs": "0.6.0",
    "simple-peer": "9.10.0",
    "socket.io-client": "4.0.0",
    "sweetalert2": "10.15.7",
    "vkey": "1.0.1",
    "webrtc-adapter": "7.7.1",
    "zipson": "0.2.12"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.1.1",
    "@angular-devkit/build-angular": "0.1102.6",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "11.2.6",
    "@angular/common": "11.2.7",
    "@angular/compiler": "11.2.7",
    "@angular/compiler-cli": "11.2.7",
    "@angular/core": "11.2.7",
    "@angular/forms": "11.2.7",
    "@angular/language-service": "11.2.7",
    "@angular/platform-browser": "11.2.7",
    "@angular/platform-browser-dynamic": "11.2.7",
    "@angular/router": "11.2.7",
    "@ionic/angular-toolkit": "^3.1.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/line-clamp": "latest",
    "@tailwindcss/typography": "latest",
    "@types/jasmine": "3.6.8",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.2.2",
    "@types/node": "14.14.36",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "chai": "4.3.4",
    "chokidar": "3.5.1",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.9.1",
    "cross-env": "7.0.3",
    "electron": "12.0.2",
    "electron-builder": "22.10.5",
    "electron-rebuild": "2.3.5",
    "electron-reload": "1.5.0",
    "eslint": "7.23.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.3.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.3.2",
    "ngx-tailwind": "2.1.0",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.6",
    "spectron": "14.0.0",
    "tailwindcss": "^2.0.4",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.1.5",
    "wait-on": "5.3.0",
    "webdriver-manager": "12.1.8",
    "zone.js": "0.11.4"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "chrome 83"
  ],
  "optionalDependencies": {
    "fsevents": "2.3.2"
  }
}
