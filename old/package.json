    "@angular/router": "11.2.6",
    "@ionic/angular": "^5.6.3",
    "@ionic/core": "^5.6.3",
    "@ionic/storage": "^3.0.2",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/mocha": "8.2.2",
    "autoprefixer": "^10.2.5",
    "build-number-generator": "1.0.0",
    "chai": "4.3.4",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "3.9.1",
    "cross-env": "^7.0.3",
    "electron": "12.0.1",
    "electron-builder": "^22.10.5",
    "electron-publisher-simple-http": "0.0.5",
